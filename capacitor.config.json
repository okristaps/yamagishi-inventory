{
  "appId": "com.yamagishi.inventory",
  "appName": "Yamagishi Inventory",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "out",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "StatusBar": {
      "style": "Dark",
      "backgroundColor": "#121212",
      "overlaysWebView": true
    },
    "CapacitorSQLite": {
      "iosDatabaseLocation": "Library/CapacitorDatabase",
      "iosIsEncryption": true,
      "iosKeychainPrefix": "yamagishi",
      "iosBiometric": {
        "biometricAuth": false,
        "biometricTitle": "Biometric login for yamagishi-inventory"
      },
      "androidIsEncryption": true,
      "androidBiometric": {
        "biometricAuth": false,
        "biometricTitle": "Biometric login for yamagishi-inventory",
        "biometricSubTitle": "Log in using your biometric"
      }
    }
  },
  "cordova": {}
}
