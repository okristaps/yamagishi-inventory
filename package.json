{
  "name": "yamagishi-inventory",
  "version": "5.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "build": "next build",
    "dev": "next dev",
    "ionic:serve": "node ionic-serve.js",
    "lint": "eslint . --ext .ts,.tsx",
    "prettify": "prettier --write **/*.tsx",
    "preserve": "npm run build",
    "serve": "npx serve out",
    "presync": "npm run build",
    "sync": "npx cap sync",
    "android": "npx cap run android",
    "android:dev": "ionic capacitor run android -l --external --livereload --consolelogs --serverlogs",
    "build:apk": "npm run build && npx cap sync && cd android && ./gradlew assembleDebug && echo 'APK generated at: android/app/build/outputs/apk/debug/app-debug.apk'",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ormconfig.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ormconfig.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ormconfig.ts",
    "schema:sync": "typeorm-ts-node-commonjs schema:sync -d ormconfig.ts",
    "schema:drop": "typeorm-ts-node-commonjs schema:drop -d ormconfig.ts"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "6.0.2",
    "@capacitor/android": "6.2.1",
    "@capacitor/core": "6.2.1",
    "@capacitor/status-bar": "6.0.3",
    "@ionic/react": "7.7.4",
    "@ionic/react-router": "7.7.4",
    "better-sqlite3": "12.6.2",
    "classnames": "2.5.1",
    "i18next": "25.7.4",
    "next": "14.2.32",
    "next-i18next": "15.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "16.5.3",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-virtuoso": "4.7.1",
    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.28"
  },
  "devDependencies": {
    "@capacitor/cli": "6.2.1",
    "@types/better-sqlite3": "7.6.13",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.25",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "autoprefixer": "10.4.18",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.3",
    "ionicons": "7.2.2",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "pullstate": "1.25",
    "reselect": "5.1.0",
    "tailwindcss": "3.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.4.2"
  }
}
