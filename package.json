{
  "name": "yamagishi-inventory",
  "version": "5.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "build": "next build",
    "dev": "next dev",
    "ionic:serve": "node ionic-serve.js",
    "lint": "eslint . --ext .ts,.tsx",
    "prettify": "prettier --write **/*.tsx",
    "preserve": "npm run build",
    "serve": "npx serve out",
    "presync": "npm run build",
    "sync": "npx cap sync",
    "android": "npx cap run android",
    "android:dev": "ionic capacitor run android -l --external --livereload --consolelogs --serverlogs",
    "build:apk": "npm run build && npx cap sync && cd android && ./gradlew assembleDebug && echo 'APK generated at: android/app/build/outputs/apk/debug/app-debug.apk'",
    "deploy": "npm run build:apk && adb install -r android/app/build/outputs/apk/debug/app-debug.apk && adb shell am start -n com.yamagishi.inventory/com.example.app.MainActivity",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ormconfig.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ormconfig.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ormconfig.ts",
    "schema:sync": "typeorm-ts-node-commonjs schema:sync -d ormconfig.ts",
    "schema:drop": "typeorm-ts-node-commonjs schema:drop -d ormconfig.ts"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "^7.0.3",
    "@capacitor/android": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/status-bar": "^8.0.0",
    "better-sqlite3": "12.6.2",
    "classnames": "2.5.1",
    "i18next": "25.7.4",
    "next": "^15.1.4",
    "next-i18next": "15.4.3",
    "normalize.css": "8.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "16.5.3",
    "react-icons": "5.5.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-virtuoso": "4.7.1",
    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.28"
  },
  "devDependencies": {
    "@capacitor/cli": "8.0.1",
    "@types/better-sqlite3": "7.6.13",
    "@types/jest": "29.5.12",
    "@types/node": "^20.17.9",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "autoprefixer": "10.4.18",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.1.4",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "pullstate": "1.25",
    "reselect": "5.1.0",
    "tailwindcss": "3.4.19",
    "ts-node": "10.9.2",
    "typescript": "^5.7.3"
  }
}
